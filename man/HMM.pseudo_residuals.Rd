## The function is currently defined as
function (x, mod)
{
    n <- length(x)
    cdists <- HMM.conditional(xc = 0:max(x), x, mod)
    cumdists <- rbind(rep(0, n), apply(cdists, 2, cumsum))
    ulo <- uhi <- rep(NA, n)
    for (i in 1:n) {
        ulo[i] <- cumdists[x[i] + 1, i]
        uhi[i] <- cumdists[x[i] + 2, i]
    }
    umi <- 0.5 * (ulo + uhi)
    npsr <- qnorm(rbind(ulo, umi, uhi))
    return(npsr)
  }
}
